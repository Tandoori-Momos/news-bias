{
    "manifest_version": 2,
    "name": "News Bias",
    "version": "0.0.1",
    "description": "We fight News Bias!",

    "browser_action": {
        "default_title": "News Bias",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs"
    ],

    "content_scripts": [{
            "js": ["scripts/main.js"],
            "matches": ["https://*/*"]
    }]
}